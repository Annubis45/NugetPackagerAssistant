<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
  <appSettings>
    <add key="nugetDirectory" value="Z:\PathTo\NugetApp" /><!-- folder containing nuget.exe -->
    <add key="outputDirectory" value="Z:\PathTo\NupkgOutput" /><!-- folder in which the .nupkg will be generated -->
    <add key="publishDirectory" value="Z:\PathTo\PublishFolder" /><!-- folder in which the .nupkg generated will be published -->
  </appSettings>
</configuration>
